@using Checkers.Domain.Entities
@model IEnumerable<Game>

@{
    ViewData["Title"] = "Privacy Policy";
}
<h1>@ViewData["Title"]</h1>

<p>Use this page to detail your site's privacy policy.</p>
@foreach(var game in Model)
{
    <div class="row">
        <span>
            @game.Id
        </span>
        <span>
            @game.P1
        </span>
        <span>
            @game.P2
        </span>
        <span>
            @game.Winner
        </span>
    </div>
}
